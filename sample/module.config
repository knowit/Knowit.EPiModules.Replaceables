<?xml version="1.0" encoding="utf-8"?>
<module>
  <dojo>
    <assemblies>
      <add assembly="Knowit.EPiModules.Replaceables.Sample" />
    </assemblies>
    <!-- Adds a mapping from knowit to ~/ClientResources/Scripts to the dojo loader configuration -->
    <paths>
      <add name="knowit.widgets" path="Scripts/Widgets" />
      <add name="knowit" path="Scripts" />
    </paths>
  </dojo>
</module>